{
    "root": true,
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "react-app",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    "quotes": ["error", "single", { "avoidEscape": true }],
    "arrow-body-style": ["error", "as-needed"],
    "no-irregular-whitespace": "off"
    }
}